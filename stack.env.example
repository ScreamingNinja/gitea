HOST=

GITEA__repository__DISABLED_REPO_UNITS=repo.issues,repo.pulls,repo.wiki,repo.projects,repo.packages,repo.actions
GITEA__repository__DEFAULT_REPO_UNITS=repo.code

GITEA__database__NAME=gitea
GITEA__database__USER=gitea
GITEA__database__PASSWD=
GITEA__database__LOG_SQL=false

GITEA__admin__DISABLE_REGULAR_ORG_CREATION=true
GITEA__admin__DEFAULT_EMAIL_NOTIFICATIONS=onmention

# docker run -it --rm gitea/gitea:1 gitea generate secret SECRET_KEY
# docker run -it --rm  gitea/gitea:1 gitea generate secret INTERNAL_TOKEN
GITEA__security__SECRET_KEY=
GITEA__security__INTERNAL_TOKEN=
GITEA__security__INSTALL_LOCK=true

GITEA__service__ENABLE_TIMETRACKING=false
GITEA__service__SHOW_REGISTRATION_BUTTON=false

GITEA__webhook__ALLOWED_HOST_LIST=external,private

GITEA__mailer__ENABLED=true
GITEA__mailer__FROM=
GITEA__mailer__MAILER_TYPE=smtp
GITEA__mailer__IS_TLS_ENABLED=true
GITEA__mailer__HOST=
GITEA__mailer__USER=
GITEA__mailer__PASSWD=

GITEA__cache__INTERVAL=743
GITEA__cache__ITEM_TTL=720

GITEA__attachment__ENABLED=false

GITEA__mirror__DEFAULT_INTERVAL=77977s
